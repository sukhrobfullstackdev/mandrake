  "scripts": {
    "analyze": "ANALYZE=true next build",
    "commit": "git-cz",
    "preinstall": "npx only-allow pnpm",
    "prepare": "panda codegen && husky install",
    "dev": "sh scripts/local-start.sh --env=dev",
    "generateI18n": "i18next-scanner",
    "local": "sh scripts/local-start.sh --env=local",
    "local:ip": "sh scripts/local-start.sh --env=ip",
    "prod": "sh scripts/local-start.sh --env=prod",
    "stage": "sh scripts/local-start.sh --env=stage",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "reset": "rimraf node_modules && rimraf styled-system && rimraf coverage",
    "test": "DOTENV_CONFIG_PATH=./.env.test jest --maxWorkers=50%",
    "test:watch": "DOTENV_CONFIG_PATH=./.env.test jest --watch --maxWorkers=25%",
    "test:ci": "DOTENV_CONFIG_PATH=./.env.test jest --runInBand",
    "test:lh": "pnpm build && lhci autorun"
  },
